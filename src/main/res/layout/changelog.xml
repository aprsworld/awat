                android:layout_marginLeft="10dp"
                android:id="@+id/changelog_build_git_hash" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingBottom="20dip">
